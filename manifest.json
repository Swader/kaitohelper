{
  "manifest_version": 3,
  "name": "Kaito Helper",
  "version": "1.0",
  "description": "Adds functionality to Kaito",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://yaps.kaito.ai/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 